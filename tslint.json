{
    "defaultSeverity": "error",
    "ignoreDefinitionFiles": false,
    "autoFixOnSave": true,
    "extends": [
        "tslint-microsoft-contrib"
    ],
    "jsRules": {},
    "rules": {
        "indent": [true, "spaces", 4],
        "no-redundant-jsdoc": true,
        "no-implicit-dependencies": false,
        "no-submodule-imports": false,
        "no-relative-imports": false
        
    },
    "rulesDirectory": [],
    "linterOptions": {
        "exclude": [
            "dist"
        ]
    }
}