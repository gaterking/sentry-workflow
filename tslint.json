{
    "defaultSeverity": "error",
    "ignoreDefinitionFiles": false,
    "autoFixOnSave": true,
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "quotemark": [true, "single"],
        "trailing-comma": [true, "never"],
        "space-before-function-paren": [true, "always"]
        
    },
    "rulesDirectory": [],
    "linterOptions": {
        "exclude": [
            "dist"
        ]
    }
}